# Environment variables for the M5 forecasting project
# Copy this file to .env and fill in your values

# Optional: Weights & Biases API key for experiment tracking
# WANDB_API_KEY=your_wandb_api_key_here

# Optional: Neptune API token for experiment tracking
# NEPTUNE_API_TOKEN=your_neptune_token_here

# Optional: Optuna database URL
# OPTUNA_DB_URL=sqlite:///optuna_studies.db

# Data directory (if different from default)
# DATA_DIR=/path/to/your/data

# Results directory (if different from default)
# RESULTS_DIR=/path/to/your/results

# Number of CPU cores to use for parallel processing
# N_JOBS=-1

# Memory limit for models (in GB)
# MEMORY_LIMIT=8
